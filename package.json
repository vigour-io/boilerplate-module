{
  "name": "blend-state-dynamo",
  "description": "Dynamo DB integration for use with vigour-state",
  "version": "0.0.13",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/blend-state-dynamo.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/blend-state-dynamo/issues"
  },
  "scripts": {
    "deploy": "echo comes soon needs ploy-task-runner",
    "start": "node bin/server.js",
    "test": "(ducktape; node test) | tap-difflet && standard",
    "standard": "standard",
    "browserstack": "browserify node_modules/browserstack-tape-reporter test -o test/bundle.js -r ./package.json:package.json && cd test && browserstack-runner | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard && npm run browserstack",
    "watch": "nodemon test | tap-difflet",
    "watch-browser": "budo ./test/index.js:bundle.js -d ./test --css test/bundle.css --pushstate --live -- -r ./package.json:package.json"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "envify": "^3.4.0",
    "postcssify": "^2.1.1"
  },
  "browser": {
    "./test/fn/server/index.js": "./test/fn/server/browser.js"
  },
  "devDependencies": {
    "vigour-util": "^3.0.0",
    "browserstack-runner": "^0.4.4",
    "browserstack-tape-reporter": "git+https://github.com/vigour-io/browserstack-tape-reporter.git",
    "budo": "^8.2.2",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "pre-commit": "^1.1.3",
    "standard": "^7.0.1",
    "tap-difflet": "0.4.0",
    "tape": "^4.6.0",
    "ducktape": "^1.0.0",
    "browserify": "^13.1.0",
    "now": "0.19.0"
  },
  "homepage": "https://github.com/vigour-io/blend-state-dynamo",
  "keywords": [
    "brisky",
    "player",
    "DOM"
  ],
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ],
  "browserify": {
    "transform": [
      "envify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
